<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BubbleChart?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.makery.address.view.ControladorGrafico">
   <children>
      <BubbleChart fx:id="bubbleChart" alternativeColumnFillVisible="true" alternativeRowFillVisible="false" title="Presupuesto">
        <xAxis>
          <NumberAxis autoRanging="false" label="Semana" lowerBound="1.0" side="BOTTOM" tickLabelFill="#4a2cb5" tickUnit="4.0" upperBound="53.0" fx:id="xAxis" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="yAxis" autoRanging="false" label="Producto" side="LEFT" tickLabelFill="#bf2b2b" tickUnit="10.0" upperBound="80.0" />
        </yAxis>
      </BubbleChart>
   </children>
</AnchorPane>
